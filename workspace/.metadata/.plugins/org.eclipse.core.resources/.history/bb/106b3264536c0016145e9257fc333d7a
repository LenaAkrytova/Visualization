import java.io.FileNotFoundException;
import java.io.PrintWriter;
import java.io.UnsupportedEncodingException;

public class Main {
	  public static void main(String args[]){
		  PrintWriter writer = null;
		try {
		  writer = new PrintWriter("E:/CS/Vizualization/workspace/VizualizationData/src/ProducedData.txt", "UTF-8");
		
		  writer.println("The first line");
		  
		  int value = 1;
		  Categories enumInstance = Categories.values()[value];
		  writer.println(enumInstance);
		  
		  writer.println("[");
		  for(int i = 0; i< 10; i++)
		  {
			String month = random();
			String day = random();
			enumInstance = randomCategory();
			writer.println("{" +
			    "\"date\": \"2016-" + month + "-" + day + "T00:00:00Z\"," +
			    "\"category\": { \"term\": \"" + enumInstance +"\" }" +
			    "},");
		  }
		 writer.println("]");
		  
		} catch (FileNotFoundException | UnsupportedEncodingException e) {
			e.printStackTrace();
		}  finally {
			writer.close();
		}
	       
	   }

	private static Categories randomCategory() {
		// TODO Auto-generated method stub
		return null;
	}

	private static String random() {
		// TODO Auto-generated method stub
		return null;
	}
}
