import java.io.FileNotFoundException;
import java.io.PrintWriter;
import java.io.UnsupportedEncodingException;

public class Main {
	  public static void main(String args[]){
		  PrintWriter writer = null;
		try {
		  writer = new PrintWriter("E:/CS/Vizualization/workspace/VizualizationData/src/ProducedData.txt", "UTF-8");
		
		  writer.println("The first line");
		  
		  int value = 1;
		  Categories enumInstance = Categories.values()[value];
		  writer.println(enumInstance);
		  
		  writer.println("[");
		  for(int i = 0; i< 10; i++)
		  {
			  writer.println("{
			    \"date\": \"2016-03-21T00:59:00Z\",
			    \"category\": { \"term\": \"MaritimeSafetyWorld\" }
			  },");
		  }
		 writer.println("]");
		  
		} catch (FileNotFoundException | UnsupportedEncodingException e) {
			e.printStackTrace();
		}  finally {
			writer.close();
		}
	       
	   }
}
